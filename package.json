    "react-addons-css-transition-group": "^15.6.0",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.4.0",
    "react-intl": "^2.4.0",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.2.2",
    "react-select": "^2.0.0",
    "react-swipeable-views": "^0.12.16",
    "react-test-renderer": "^16.4.1",
    "recompose": "^0.27.0",
